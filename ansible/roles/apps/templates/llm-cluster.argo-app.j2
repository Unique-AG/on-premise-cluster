apiVersion: argoproj.io/v1alpha1
kind: Application
metadata:
  name: llm-cluster
  namespace: argocd
spec:
  project: default
  destination:
    server: https://kubernetes.default.svc
    namespace: ray
  sources:
    - repoURL: https://github.com/Unique-AG/ray-cluster
      path: llm-cluster/argo
      targetRevision: HEAD